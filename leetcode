no:26

class Solution {
    public int removeDuplicates(int[] nums) {
        int k=0;
        for(int i:nums){
            if(k==0 || i!=nums[k-1]){
                nums[k]=i;
                k++;
            }
        }
        return k;
    }
}

no:27

class Solution {
    public int removeElement(int[] nums, int val) {
        int k=0;
        for(int i:nums){
            if (i!=val){
                nums[k]=i;
                k++;
            }
        }
        return k;
    }
}
